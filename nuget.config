<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="github" value="https://nuget.pkg.github.com/gpproton/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <github>
      <add key="Username" value="gpproton" />
      <add key="ClearTextPassword" value="NUGET_AUTH_TOKEN" />
    </github>
  </packageSourceCredentials>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="github">
      <package pattern="Proton.Common" />
      <package pattern="Proton.Common.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
